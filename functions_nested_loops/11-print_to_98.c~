#include "main.h"
#include <stdio.h>

/**
 *print_to_98 - prints numbers from 0 to 98 with commas and space
 *
 *Return: returns 0 if successful
 */

void print_to_98(int n)
{
  for (n = 0; n <= 98; n++)
    {
      if (n < 98)
	{
	  n++;
	  printf(n", ");
	}
      else if (n > 98)
      {
	n--;
	printf(n",");
      }
      else
	{
	  printf("98");
	}
    }
}
