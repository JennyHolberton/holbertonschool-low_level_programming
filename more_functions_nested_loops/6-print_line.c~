#include "main.h"
/**
 *print_line - prints an _ character followed by a new line
 *
 *Return: returns the _ characters
 */

void print_line(int n)
{
  int i;
  
  i = '_';
  if (n <= 1)
      {
       _putchar(i);
       n++;
      }
  if (n <= 0)
    {
     _putchar('\n');
    }
  _putchar('\n');
}
