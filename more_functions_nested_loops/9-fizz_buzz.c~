#include<stdio.h>
/**
 *main - display fizz buzz
 *
 *Return: nothing 0
 */

int main(void)
{
	int i;

	i = 1;

	while(i <= 100)
	{
		i++;
		if(i % 3 == 0 && i % 5 == 0)
		{
			printf("Fizzbuzz");
		}
		else if(i % 3 == 0)
		{
			printf("Fizz");
		}
		else if(i % 5 == 0)
		{
			printf("Buzz");
		}
		else
		{
			printf("%i", i);
		}
		if (i != 100)
			printf(" ");
	}
	printf("\n");
	return (0);
}
